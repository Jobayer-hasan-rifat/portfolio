{
  "version": 2,
  "name": "jobayer-portfolio",
  "_comment": "Static site deployment - no builds needed",
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
